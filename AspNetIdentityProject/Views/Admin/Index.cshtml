@model List<AppUser>
@{
    ViewData["Title"] = "Index";
}

    <table class="table table border table-striped">
        <tr>
            <td>User Id</td>
            <td>User Name</td>
            <td>Email</td>
        </tr>

        @if (Model.Count == 0)
        {
            <tr>
                <td colspan="3"> Kayıtlı Üye Yok.</td>
            </tr>
        }
        else
        {
            foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.UserName</td>
                    <td>@item.Email</td>
                   
                </tr>
            }
        }
    </table>

